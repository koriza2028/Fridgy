{
  "expo": {
    "name": "Fridgy",
    "owner": "fridgyminds",
    "slug": "Fridgy",
    "scheme": "fridgy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "deepLinking": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CreativeMinds.Fridgy",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "fridgy"
            ]
          }
        ]
      },
      "appleTeamId": "SY9H89HY87"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.CreativeMinds.Fridgy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "ddd686eb-12a8-4392-a854-0393f7ef8ede"
      }
    }
  }
}
